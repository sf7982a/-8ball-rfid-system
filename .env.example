# 8Ball RFID Environment Variables Template
# Copy this file to .env.local for local development

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application Configuration
VITE_APP_ENV=development
VITE_APP_NAME=8Ball RFID (Local)
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_RFID_SIMULATION=true
VITE_ENABLE_DEBUG_MODE=true

# Optional URLs
VITE_API_URL=
VITE_DASHBOARD_URL=
VITE_SUPPORT_URL=mailto:support@8ball-rfid.com

# Development Notes:
# - For local development, use your local/development Supabase project
# - For staging, Vercel will use staging environment variables
# - For production, Vercel will use production environment variables
# - Never commit actual keys to git - use Vercel dashboard for staging/production